    <!-- 完全退出使用的权限 -->
    <uses-permission android:name="android.permission.RESTART_PACKAGES" />
    <!-- 检查网络状态的权限 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 使用网络的权限 -->
